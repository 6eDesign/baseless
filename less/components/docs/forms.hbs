<form class="{{cssPrefix}}form">
  <div class="{{cssPrefix}}form-group">
    <label>Text Form Example</label>
    <input type="text" class="{{cssPrefix}}form-elem" placeholder="Placeholder text.."/>
    <button class="{{cssPrefix}}btn warning" type="button">Submit</button>
    <div class="{{cssPrefix}}form-descriptor">
      Some sample stuff here
    </div>
  </div>
  <div class="{{cssPrefix}}form-group">
    <label>Textarea Form Example</label>
    <textarea class="{{cssPrefix}}form-elem"></textarea>
  </div>
  <div class="{{cssPrefix}}form-group">
    <label>Select Form Example</label>
    <select class="{{cssPrefix}}form-elem">
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
      <option value="null">Example Option</option>
    </select>
    <button class="{{cssPrefix}}btn warning" type="button">Submit</button>
  </div>
  <div class="{{cssPrefix}}form-group inline">
    <div class="row">
      <div class="col threecol">
        <input type="radio" value="male" id="male" name="sex"/>
        <label class="{{cssPrefix}}form-gap-l" for="male">Male</label>
      </div>
      <div class="col threecol">
        <input type="radio" value="female" id="female" name="sex"/>
        <label class="{{cssPrefix}}form-gap-l" for="female">Female</label>
      </div>
      <div class="col threecol">
        <label class="{{cssPrefix}}form-gap-r" for="male2">Male</label>
        <input type="checkbox" value="male2" id="male2" name="sex2"/>
      </div>
      <div class="col threecol">
        <label class="{{cssPrefix}}form-gap-r" for="female2">Female</label>
        <input type="checkbox" value="female2" id="female2" name="sex2"/>
      </div>
    </div>
  </div>
</form>
